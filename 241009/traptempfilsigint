#!/bin/bash

# trap f책ngar "signaler" till processen

# Vi anv채nder en tempor채r fil

touch mintempfil

# Utan exit kan vi blockera avst채ngningen
trap 'rm -v mintempfil; exit 0' SIGINT

for ((count = 0; count < 10000; count++)); do
    echo "$RANDOM" >>mintempfil
    sleep 1
done

sort -n mintempfil | head
